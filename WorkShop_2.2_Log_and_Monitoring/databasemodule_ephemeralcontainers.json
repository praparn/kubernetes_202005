{
    "apiVersion": "v1",
    "kind": "EphemeralContainers",
    "metadata": {
      "name": "podsname",
      "labels": {
        "name": "maindb",
        "owner": "Praparn_L",
        "version": "1.0",
        "module": "maindb",
        "environment": "development"
    }
  },
    "ephemeralContainers": [{
        "command": [
            "sh"
        ],
        "image": "alpine:latest",
        "imagePullPolicy": "IfNotPresent",
        "name": "debugger",
        "stdin": true,
        "tty": true,
        "terminationMessagePolicy": "File"
    }]
}